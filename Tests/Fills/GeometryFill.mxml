<?xml version="1.0" encoding="utf-8"?>
<mx:Application xmlns:mx="http://www.adobe.com/2006/mxml" 
layout="absolute"
xmlns="http://www.degrafa.com/2007"
xmlns:flashfilters="flash.filters.*"
>
<mx:Canvas width="200" height="322" borderStyle="solid" borderColor="#CCCCCC"
	 id="sourceGeometry" x="10" y="220">
		 <mx:Label text="Source Geometry"  x="5" y="5"/>
		 	<mx:Label text="Ellipse width"  x="5" y="25"/>
	<mx:HSlider id="EllipseWidth" minimum="1" maximum="100" value="50" 
		liveDragging="true" snapInterval="1" tickInterval="2" x="5" y="45"/>
				 	<mx:Label text="Ellipse height"  x="5" y="65"/>
	<mx:HSlider id="EllipseHeight" minimum="1" maximum="100" value="50" 
		liveDragging="true" snapInterval="1" tickInterval="2" x="5" y="85"/>
			<mx:Label text="Ellipse x"  x="5" y="105"/>
	<mx:HSlider id="EllipseX" minimum="-10" maximum="100" value="0" 
		liveDragging="true" snapInterval="1" tickInterval="2" x="5" y="125"/>
			<mx:Label text="Ellipse y"  x="5" y="145"/>
	<mx:HSlider id="EllipseY" minimum="-10" maximum="100" value="0" 
		liveDragging="true" snapInterval="1" tickInterval="2" x="5" y="165"/>
		 <mx:Canvas id="sourceRender" x="5" y="185" />
		  <mx:Canvas id="clipRectRender" x="5" y="185" />
	</mx:Canvas>	
	<mx:Canvas width="500" height="322" borderStyle="solid" borderColor="#CCCCCC"
	 id="aTarget" x="220" y="220">
		 <mx:Label text="GeometryFillDemo, RegularRectangle target"  x="5" y="5"/>
		  <mx:Label text="Reference Ellipse with gradient transform"  x="260" y="5"/>
		 <mx:Label text="RegularRectangle width"  x="5" y="25"/>
	<mx:HSlider id="RegularRectangleWidth" minimum="1" maximum="400" value="250" 
		liveDragging="true" snapInterval="2" tickInterval="10" x="5" y="55"/>
				 	<mx:Label text="RegularRectangle height"  x="5" y="75"/>
	<mx:HSlider id="RegularRectangleHeight" minimum="1" maximum="200" value="100" 
		liveDragging="true" snapInterval="2" tickInterval="10" x="5" y="95"/>
		<mx:Label text="RegularRectangle stroke weight"  x="5" y="115"/>
	<mx:HSlider id="RegularRectangleStroke" minimum="0" maximum="30" value="0" 
		liveDragging="true" snapInterval="1" tickInterval="1" x="5" y="135"/>
		 <mx:Canvas id="fillRender" x="5" y="165" />
	</mx:Canvas>
	<mx:Label text="Y"  x="68" y="10"/>
	<mx:VSlider id="transY" minimum="-200" maximum="200" value="0" 
		liveDragging="true" snapInterval="1" x="64" y="35"/>
	
	<mx:Label text="X"  x="33" y="10"/>
	<mx:VSlider id="transX" minimum="-200" maximum="200" value="0" 
		liveDragging="true" snapInterval="1" x="29" y="35"/>
	
	<mx:Label text="Angle"  x="91" y="10"/>
	<mx:VSlider id="angle" minimum="0" maximum="360" value="0" 
		liveDragging="true" snapInterval="1" x="98" y="35"/>
	
	<mx:Label text="ScaleY"  x="184" y="10"/>
	<mx:VSlider id="ascaleY" minimum="-2" maximum="2" value="1" 
		liveDragging="true" snapInterval=".01" tickInterval="0.01" x="193" y="35"/>
	
	<mx:Label text="Scale X"  x="134" y="10"/>
	<mx:VSlider id="ascaleX" minimum="-2" maximum="2" value="1" 
		liveDragging="true" snapInterval=".01" tickInterval="0.01" x="148" y="35"/>
	
	
	<mx:Label text="Skew Y"  x="284" y="10"/>
	<mx:VSlider id="skewY" minimum="-90" maximum="90" value="0" 
		liveDragging="true" snapInterval=".01" tickInterval="0.01" x="293" y="35"/>
	
	<mx:Label text="Skew X"  x="234" y="10"/>
	<mx:VSlider id="skewX" minimum="-90" maximum="90" value="0" 
		liveDragging="true" snapInterval=".01" tickInterval="0.01" x="248" y="35"/>
	<mx:ColorPicker visible="{enableBG.selected}" id="geoFillBackground" x="560" y="40" />
	<mx:CheckBox x="590" label="BG Fill enabled" y="{geoFillBackground.y}" id="enableBG" selected="false" />
<mx:CheckBox x="590" label="enable Filters" y="{enableBG.y+20}" id="enableFilters" selected="false" />

	<mx:Label text="Fill transform Registration Point" x="399" y="12"/>
	<mx:ComboBox id="regPoint" labelField="label" 
		creationComplete="regPoint.selectedIndex=4" x="399" y="40">
		<mx:ArrayCollection>
        	<mx:Object label="topLeft"/>
    	    <mx:Object label="centerLeft"/>
	        <mx:Object label="bottomLeft"/>
	        
	        <mx:Object label="centerTop"/>
	        <mx:Object label="center"/>
	        <mx:Object label="centerBottom"/>
	        
	        <mx:Object label="topRight"/>
	        <mx:Object label="centerRight"/>
	        <mx:Object label="bottomRight"/>
      	</mx:ArrayCollection>
	</mx:ComboBox>
	<mx:Label text="Repeat X" x="700" y="12"/>
	<mx:ComboBox id="repeatXValue" labelField="data" enabled="{targetSetting.selectedIndex==0}"
		creationComplete="repeatXValue.selectedIndex=0" x="700" y="40">
		<mx:ArrayCollection>
        	<mx:Object data="none"/>
    	    <mx:Object data="space"/>
	        <mx:Object data="repeat"/>
	        <mx:Object data="stretch"/>
      	</mx:ArrayCollection>
	</mx:ComboBox>
	<mx:Label text="Repeat Y" x="700" y="70"/>
	<mx:ComboBox id="repeatYValue" labelField="data" enabled="{targetSetting.selectedIndex==0}"
		creationComplete="repeatYValue.selectedIndex=0" x="700" y="100">
		<mx:ArrayCollection>
        	<mx:Object data="none"/>
    	    <mx:Object data="space"/>
	        <mx:Object data="repeat"/>
	        <mx:Object data="stretch"/>
      	</mx:ArrayCollection>
	</mx:ComboBox>
	<mx:Button x="{transX.x}" y="{transX.y+transX.height+2}" label="Reset Transform" click="{theTransform.suppressEventProcessing=true;ascaleX.value=1;ascaleY.value=1;angle.value=0;skewX.value=0;skewY.value=0;transX.value=0;transY.value=0;theTransform.suppressEventProcessing=false;}" />
	<mx:Label text="GeometryFill targetSetting" x="399" y="{regPoint.y+regPoint.height+10}"/>
	<mx:ComboBox id="targetSetting" labelField="label" 
		creationComplete="targetSetting.selectedIndex=0" x="399" y="{regPoint.y+regPoint.height+30}">
		<mx:ArrayCollection source="{com.degrafa.paint.GeometryFill.targetSettingOptions}" />
	</mx:ComboBox>
<mx:CheckBox x="399" label="GeometryFill insetAgainstStroke" y="{regPoint.y+regPoint.height+60}" id="insetByStroke" selected="false" />
<mx:CheckBox x="399" label="GeometryFill enableSourceClipping" y="{regPoint.y+regPoint.height+80}" id="sourceClipping" selected="false" />

<mx:Label id="clipXLabel" text="clipsource x"  x="399" y="{regPoint.y+regPoint.height+100}" enabled="{sourceClipping.selected}" />
	<mx:HSlider enabled="{sourceClipping.selected}" id="clipX" minimum="-10" maximum="200" value="10" 
		liveDragging="true" snapInterval="2" tickInterval="5" x="{clipXLabel.x+clipXLabel.width+10}" y="{regPoint.y+regPoint.height+100}"/>
<mx:Label id="clipYLabel" text="clipsource y"  x="399" y="{regPoint.y+regPoint.height+120}" enabled="{sourceClipping.selected}" />
	<mx:HSlider enabled="{sourceClipping.selected}" id="clipY" minimum="-10" maximum="200" value="10" 
		liveDragging="true" snapInterval="2" tickInterval="5" x="{clipYLabel.x+clipYLabel.width+10}" y="{regPoint.y+regPoint.height+120}"/>

<GeometryComposition id="fillComposition" graphicsTarget="{[sourceRender]}" autoClearGraphicsTarget="true" >
	<!--	<Circle id="circleFill" centerX="10" centerY="10" radius="{angle.value+5}" stroke="{solStroke}" fill="{gradientFills7}" />
	-->
	<Ellipse x="{EllipseX.value}" y="{EllipseY.value}" width="{EllipseWidth.value}" height="{EllipseHeight.value}" id="sourceGeom">
				<fill>
					<LinearGradientFill>
						<GradientStop color="0xFF0000"/>
						<GradientStop color="0xFFFF00"/>
					</LinearGradientFill>
				</fill>
			</Ellipse>
				<RegularRectangle x="25" y="25" width="40" height="10">
					<fill>
						<SolidFill color="#00aacc" alpha=".5" />
					</fill>
				</RegularRectangle>
		
	</GeometryComposition>
	
