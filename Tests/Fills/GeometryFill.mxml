	<GeometryComposition graphicsTarget="{[fillRender]}">
	<RegularRectangle  x="10" y="10" width="{RegularRectangleWidth.value}" height="{RegularRectangleHeight.value}">
		<stroke>
			<SolidStroke color="0x000000" weight="{RegularRectangleStroke.value}" />
		</stroke>
		<fill>
	<GeometryFill source="{fillComposition}" enableFilters="{enableFilters.selected}" enableBackground="{enableBG.selected}" clipSource="{clipsourceGeom}" enableSourceClipping="{sourceClipping.selected}" insetFromStroke="{insetByStroke.selected}" targetSetting="{targetSetting.selectedItem}" repeatX="{repeatXValue.selectedItem.data}" repeatY="{repeatYValue.selectedItem.data}" transform="{theTransform}">
	<solidFillBackground>
		<SolidFill id="geometryBackGround" color="{geoFillBackground.selectedColor}" alpha="1" />
	</solidFillBackground>
	<filters>
		<flashfilters:BlurFilter blurX="14" blurY="14"/>
		<flashfilters:DropShadowFilter  />
	</filters>
	</GeometryFill>		
		</fill>
		<!-- Child geometry -->
		<geometry>
			<Ellipse  x="280" y="-110" width="200" height="100" fill="{anotherGradient}">
			</Ellipse> 
		</geometry>
	</RegularRectangle>
	</GeometryComposition>
	<GeometryComposition id="clipsourceGeom" graphicsTarget="{[clipRectRender]}" autoClearGraphicsTarget="true">
			<RegularRectangle visible="{sourceClipping.selected}"  x="{clipX.value}" y="{clipY.value}" width="80" height="40">
				<stroke>
					<SolidStroke color="black" weight="0" />
				</stroke>
			</RegularRectangle>
	</GeometryComposition>
<!--	</Path>-->
	<Transform id="theTransform" registrationPoint="{regPoint.selectedItem.label}" scaleX="{ascaleX.value}" scaleY="{ascaleY.value}"  angle="{angle.value}"  skewY="{skewY.value}" skewX="{skewX.value}"  x="{transX.value}"  y="{transY.value}" />
	<Transform id="gradientTransform"  angle="{angle.value}" scaleX="{ascaleX.value}" scaleY="{ascaleY.value}" skewY="{skewY.value}" skewX="{skewX.value}"  x="{transX.value}"  y="{transY.value}" />
<RadialGradientFill focalPointRatio="1" transform="{theTransform}"   id="anotherGradient" angle="45">
                <GradientStop alpha=".3" color="#ff00CD"/>
                <GradientStop alpha=".8" color="#000033"/>
				<GradientStop alpha="1" color="#FFFFFF"/>
</RadialGradientFill>
	<SolidStroke id="solStroke" color="#ffffff" weight="4" />
		<SolidFill id="solFill" color="#000000" alpha="1" />
	      <LinearGradientFill id="gradientFills7" transform="{theTransform}" angle="91.4251" spreadMethod="pad" interpolationMethod="rgb" >
         <GradientStop color="#ffffff" alpha="1" ratio="0" />
         <GradientStop color="#03070b" alpha="0.654822" ratio="1" />
      </LinearGradientFill>
</mx:Application>
