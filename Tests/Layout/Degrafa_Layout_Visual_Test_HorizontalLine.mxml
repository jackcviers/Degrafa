<?xml version="1.0" encoding="utf-8"?>
<mx:Application xmlns:mx="http://www.adobe.com/2006/mxml" layout="absolute"
	xmlns="http://www.degrafa.com/2007">
	
	<HorizontalLine graphicsTarget="{[target1]}" left="5" top="5" bottom="5" right="5">
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target2]}" percentWidth=".75">
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target3]}" left="10" top="10" width="50">
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target4]}" width="50" verticalCenter="0" horizontalCenter="0" >
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
		
	<HorizontalLine graphicsTarget="{[target6]}" verticalCenter="0" percentWidth=".5">
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target7]}" horizontalCenter="0" percentWidth=".75">
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target8]}" percentWidth=".5" bottom="5" right="5" >
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target9]}" percentWidth=".75" maintainAspectRatio="false" top="15" left="5" >
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target10]}" percentWidth=".75" bottom="0" horizontalCenter="0">
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
		
	<HorizontalLine graphicsTarget="{[target12]}" x="25" y="25" width="50" >
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target13]}" x="20" y="20" width="50%" >
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<HorizontalLine graphicsTarget="{[target14]}" percentWidth="75">
		<stroke>
			<SolidStroke weight="2" color="green"/>
		</stroke> 		
	</HorizontalLine>
	
	<mx:Canvas id="target1" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="10" y="10"/>
	<mx:Label x="118" y="10" text="left =5 top=5 right=5 bottom=5" width="294" height="100"/>
		
	<mx:Canvas id="target2" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="10" y="120"/>
	<mx:Label x="118" y="120" text="percentWidth =.75" width="294" height="100"/>
	
	<mx:Canvas id="target3" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="10" y="230"/>
	<mx:Label x="118" y="230" text="left=10 top=10 width=50" width="294" height="100"/>
	
	<mx:Canvas id="target4" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="10" y="340"/>
	<mx:Label x="118" y="340" text="width=50 verticalCenter=0 horizontalCenter=0" width="333" height="100"/>
	
	<mx:Canvas id="target6" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="10" y="448"/>
	<mx:Label x="118" y="448" text="percentWidth =.5 verticalCenter=0" width="325" height="100"/>
	
	<mx:Canvas id="target7" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="10" y="558"/>
