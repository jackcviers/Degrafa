	<mx:Canvas id="target9" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="483" y="120"/>
	<mx:Label x="591" y="120" text="{'percentHeight =.5 maintainAspectRatio=flase' + '\n' + 'top=15 left =5'}" width="388" height="100"/>
	
	<mx:Canvas id="target10" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="483" y="230"/>
	<mx:Label x="591" y="230" text="{'percentHeight=.75 bottom=0' + '\n' + 'horizontalCenter=0'}" width="388" height="100"/>
		
	<mx:Canvas id="target12" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="483" y="340"/>
	<mx:Label x="591" y="340" text="x=25 y=25 height=50" width="388" height="100"/>
	
	<mx:Canvas id="target13" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="483" y="450"/>
	<mx:Label x="591" y="450" text="x=20 y=20 height=50%" width="388" height="100"/>
	
	<mx:Canvas id="target14" borderColor="#666666" borderStyle="solid" 
		width="100" height="100" x="483" y="560"/>
	<mx:Label x="591" y="560" text="percentHeight =75" width="388" height="100"/>
	
	
</mx:Application>
